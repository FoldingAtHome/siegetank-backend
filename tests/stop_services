#! /usr/bin/env python3

import os
import signal
import time

pids = open('pids.log').read().split(' ')

for pid in pids:
    try:
        os.killpg(int(pid), signal.SIGTERM)
    except Exception:
        pass

# avoid weird error where a file is being written while an rm rf is being
# issued
time.sleep(1)

os.system('rm -rf *.log *_data')
